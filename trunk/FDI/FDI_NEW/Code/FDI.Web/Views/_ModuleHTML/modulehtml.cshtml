@model FDI.Simple.HtmlSettingItem
@if (Model != null)
{
    if (@Model.Ishow)
    {
    <div style="border: 1px solid red; overflow: hidden">
        <a class="add button SuaNoiHung btn btn-primary" style="position: absolute; z-index: 1000;" id="btnSuaNoiHung" href="@Url.Action("Index", "HtmlSettings", new { key = Model.Key })" target="_black" >Sửa nội dung</a>
        @Html.Raw(Model.Value)
    </div>
    }
    else
    {
    @Html.Raw(Model.Value)
    }
}
