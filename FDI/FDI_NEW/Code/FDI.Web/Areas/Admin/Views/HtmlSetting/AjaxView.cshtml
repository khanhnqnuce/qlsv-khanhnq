@model FDI.Base.HtmlSetting

@{
    ViewBag.Title = "Xem chi tiết";
    Layout = "~/Areas/Admin/Views/Shared/_Ajax.cshtml";
}
<fieldset class="forms">
    <table class="tableforms tabledetails" cellspacing="1" width="100%">
        <tr>
            <td style="width:80px;" class="label"><label>Tên Công ty</label></td>
            <td>
                @Html.DisplayFor(model => model.Key)
            </td>
        </tr>

        <tr>
            <td class="label"><label>Địa chỉ</label></td>
            <td>
                @Html.DisplayFor(model => model.Value)
            </td>
        </tr>
    </table>
</fieldset>