@model FDI.Simple.ActiveRoleItem
@{
    ViewBag.Title = "AjaxView";
 Layout = "~/Areas/Admin/Views/Shared/_Ajax.cshtml";
}
<fieldset class="forms">
    <table class="tableforms tabledetails" cellspacing="1" width="100%">
        <tr>
            <td style="width: 100px;">
                <label>Tên chức năng</label>
            </td>
            <td>
                @Html.DisplayFor(model => model.NameActive)
            </td>
        </tr>

        <tr>
            <td style="width: 100px;">
                <label>Thứ tự</label>
            </td>
            <td>
                @Html.DisplayFor(model => model.Ord)
            </td>
        </tr>
    </table>
</fieldset>