@{
    Layout = null;
}
@model List<FDI.Simple.NewsItem>
@if (Model.Count > 0)
{
    <div class="wdc-ct-morenews">
        <div class="title-newsmore"><a href="#">Tin tức khác</a></div>
        <ul>
            @foreach (var item in Model)
            {
                 <li><a href="/@item.TitleAscii">@item.Title</a></li>
            }
           
            
        </ul>
    </div>
}
